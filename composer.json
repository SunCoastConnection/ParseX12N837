{
    "name": "suncoast-connection/parsex12n837",
    "description": "Parse X12N837 files.",
    "type": "project",
    "authors": [
        {
            "name": "Sam Likins",
            "email": "sam.likins@wsi-services.com",
            "role": "Developer"
        }
    ],
    "require": {
        "suncoast-connection/parsex12": "dev-master"
   	},
    "require-dev": {
        "laravel/homestead": "^4",
        "phploc/phploc": "3.0.*",
        "phpunit/phpunit": "5.3.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "SunCoastConnection\\ParseX12N837\\": "source/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SunCoastConnection\\ParseX12N837\\Tests\\": "tests/source"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/SunCoastConnection/ParseX12.git"
        }
    ]
}
